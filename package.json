{
  "name": "@docus/core",
  "version": "1.2.3",
  "description": "The Docus core.",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module"
  ],
  "repository": "https://github.com/docusgen/core",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.mjs",
    "./runtime": "./dist/runtime.mjs",
    "./dist/runtime/*": "./dist/runtime/*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./types.d.ts",
  "files": [
    "dist",
    "types.d.ts",
    "shims.d.ts",
    "components",
    "runtime.mjs",
    "runtime.cjs",
    "runtime.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nuxt dev example",
    "generate": "nuxt generate example",
    "start": "nuxt start example",
    "lint": "eslint --ext .js,.ts,.vue .",
    "test": "yarn lint && mocha  --exit -r jiti/register test/**/*.test.ts",
    "clean:example": "rm -rf example/.nuxt example/node_modules",
