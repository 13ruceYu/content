    "babel-jest": "latest",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "latest",
    "nuxt-edge": "^2.16.0-27257777.293dc11a",
    "playwright": "^1.16.2",
