{
  "name": "@docus/content",
  "version": "0.0.14",
  "description": "Temporary @nuxt/content module for Nuxt 3.",
  "author": "NuxtLabs <hello@nuxtlabs.com>",
  "license": "ISC",
  "homepage": "https://github.com/docusgen/docus#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docusgen/docus.git"
  },
  "keywords": [
    "nuxt",
    "content",
    "module"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "type": "module",
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/runtime/types.d.ts",
  "scripts": {
    "build": "nuxt-module-build",
    "build:docs": "nuxi build docs",
    "build:examples": "nuxi build examples",
    "dev": "nuxi dev docs",
    "dev:examples": "nuxi dev examples",
    "start:docs": "nuxi preview docs",
    "start:examples": "nuxi dev examples",
    "lint": "eslint --ext .js,.ts,.vue .",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue .",
    "test": "yarn build:docs && yarn test:unit && yarn test:e2e",
    "test:types": "tsc --build tsconfig.json",
    "test:unit": "vitest run --config test/vite.config.js",
    "test:e2e": "start-server-and-test 'yarn dev' http://localhost:3000 'yarn cy:run'",
    "test:e2e:gui": "start-server-and-test 'yarn dev' http://localhost:3000 'yarn cy:open'",
    "coverage": "vitest --coverage",
    "prepare": "husky install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "benchmark": "node --loader ts-node/esm test/benchmarks/run.ts test/benchmarks/*.benchmark.*",
    "version:bump-edge": "yarn jiti .github/scripts/bump-edge.ts",
    "prepack": "nuxt-module-build"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --fix"
  },
  "dependencies": {
    "@docus/remark-mdc": "npm:@docus/remark-mdc-edge@latest",
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest",
    "consola": "^2.15.3",
    "debounce": "^1.2.1",
    "defu": "^5.0.1",
    "detab": "^3.0.1",
    "escape-html": "^1.0.3",
    "flat": "^5.0.2",
    "gray-matter": "^4.0.3",
    "html-tags": "^3.1.0",
    "pathe": "^0.2.0",
    "prismjs": "^1.26.0",
    "property-information": "^6.1.1",
    "rehype-raw": "^6.1.1",
    "rehype-sort-attribute-values": "^4.0.0",
    "rehype-sort-attributes": "^4.0.0",
    "remark-autolink-headings": "^7.0.1",
    "remark-emoji": "^3.0.2",
    "remark-external-links": "^9.0.1",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-slug": "^7.0.1",
    "remark-squeeze-paragraphs": "^5.0.1",
    "ufo": "^0.7.10",
    "unified": "^10.1.1",
    "unstorage": "^0.3.3",
    "ws": "^8.4.2"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.1.7",
    "@nuxt/test-utils": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/benchmark": "^2.1.1",
    "@types/debounce": "^1.2.1",
    "@types/faker": "^6.6.8",
    "@types/flat": "^5.0.2",
    "@types/object-hash": "^2.2.1",
    "@types/ws": "^8.2.2",
    "benchmark": "^2.1.4",
    "c8": "^7.11.0",
    "cypress": "^9.4.1",
    "eslint": "^8.8.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "execa": "^6.0.0",
    "faker": "^6.6.6",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "nuxt3": "3.0.0-27403830.c1148d4",
    "object-hash": "^2.2.0",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "vitest": "^0.2.7"
  },
  "bugs": {
    "url": "https://github.com/docusgen/docus/issues"
  }
}
