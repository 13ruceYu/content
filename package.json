{
  "name": "@docus/core",
  "version": "1.1.1",
  "description": "The Docus core.",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module"
  ],
  "repository": "https://github.com/docusgen/core",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./runtime": {
      "require": "./dist/runtime.js",
      "import": "./dist/runtime.mjs"
    },
    "./runtime/*": "./dist/runtime/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "runtime.mjs",
    "runtime.js",
    "runtime.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nuxt dev example",
    "generate": "nuxt generate example --force-build",
    "start": "nuxt start example",
    "lint": "eslint --ext .js,.ts,.vue .",
    "test": "yarn lint && jest",
    "clean:example": "rm -rf example/.nuxt example/node_modules",
    "clean:app": "rm -rf src/app/node_modules src/app/.nuxt",
    "clean:dist": "rm -rf dist node_modules",
    "clean": "yarn clean:example && yarn clean:app && yarn clean:dist"
  },
  "dependencies": {
    "@lokidb/loki": "^2.1.0",
    "@nuxt/kit": "^0.6.4",
    "debounce": "^1.2.1",
    "defu": "^5.0.0",
    "detab": "^3.0.0",
    "escape-html": "^1.0.3",
    "flat": "^5.0.2",
    "gray-matter": "^4.0.3",
    "h3": "^0.2.12",
    "hasha": "^5.2.2",
    "html-tags": "^3.1.0",
    "jiti": "^1.11.0",
    "mdast-util-to-hast": "^11.2.0",
    "mdast-util-to-markdown": "0.6.5",
    "mdurl": "^1.0.1",
    "ohmyfetch": "^0.2.0",
    "parse-entities": "^2.0.0",
    "prismjs": "^1.24.1",
    "property-information": "5.6.0",
    "rehype-raw": "^5.1.0",
    "rehype-sort-attribute-values": "^3.0.2",
    "rehype-sort-attributes": "^3.0.2",
    "remark-autolink-headings": "^6.0.1",
    "remark-emoji": "^2.2.0",
    "remark-external-links": "^8.0.0",
    "remark-footnotes": "^3.0.0",
    "remark-gfm": "^1.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "remark-slug": "^6.0.0",
    "remark-squeeze-paragraphs": "^4.0.0",
    "scule": "^0.2.1",
    "stringify-entities": "3.1.0",
    "unctx": "^1.0.1",
    "unified": "^9.2.1",
    "unist-builder": "^3.0.0",
    "unist-util-position": "^4.0.1",
    "unist-util-visit": "2.0.3",
    "unist-util-visit-parents": "3.1.1",
    "unstorage": "^0.2.3",
    "upath": "^2.0.1",
    "vue-docgen-api": "^4.40.0",
    "ws": "^7.5.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-runtime": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-typescript": "latest",
    "@docus/app": "^1.1.2",
    "@nuxt/test-utils": "latest",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/debounce": "^1.2.0",
    "@types/escape-html": "^1.0.1",
    "@types/flat": "^5.0.2",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "latest",
    "@types/prismjs": "^1.16.6",
    "@types/unist": "^2.0.6",
    "@types/ws": "^7.4.7",
    "babel-jest": "latest",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "latest",
    "playwright": "^1.13.1",
    "prettier": "^2.3.2",
    "unbuild": "^0.4.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
