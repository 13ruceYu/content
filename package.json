    "remark-directive": "^1.0.1",
    "remark-emoji": "^2.2.0",
    "remark-external-links": "^8.0.0",
    "remark-footnotes": "^3.0.0",
    "remark-gfm": "^1.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
