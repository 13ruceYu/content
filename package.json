  "exports": {
    ".": "./dist/index.mjs",
    "./runtime": "./dist/runtime.mjs",
    "./dist/runtime/*": "./dist/runtime/*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./types.d.ts",
  "files": [
    "dist",
    "types.d.ts",
    "shims.d.ts",
    "components",
    "runtime.mjs",
    "runtime.cjs",
    "runtime.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nuxt dev example",
    "generate": "nuxt generate example",
    "start": "nuxt start example",
    "lint": "eslint --ext .js,.ts,.vue .",
    "test": "yarn lint && jest --forceExit",
