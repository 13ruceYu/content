{
  "name": "docus",
  "version": "3.0.0",
  "description": "The Modern Website Generator.",
  "author": "NuxtLabs <hello@nuxtlabs.com>",
  "license": "ISC",
  "homepage": "https://github.com/docusgen/docus#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docusgen/docus.git"
  },
  "keywords": [
    "nuxt",
    "module",
    "content-driven",
    "website-generator"
  ],
  "files": [
    "dist",
    "types.d.ts",
    "env.d.ts"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./types.d.ts",
  "exports": {
    ".": "./dist/index.mjs",
    "./runtime": "./dist/runtime/index.mjs",
    "./dist/runtime/*": "./dist/runtime/*"
  },
  "scripts": {
    "build": "unbuild",
    "build:docs": "nuxi build docs",
    "build:theme": "nuxi build theme",
    "build:examples": "nuxi build theme",
    "dev": "nuxi dev docs",
    "dev:theme": "nuxi dev theme",
    "dev:examples": "nuxi dev examples",
    "start:docs": "nuxi preview docs",
    "start:theme": "nuxi preview theme",
    "start:examples": "nuxi dev examples",
    "lint": "eslint --ext .js,.ts,.vue .",
    "test": "pnpm build:docs && pnpm test:unit && pnpm test:e2e",
    "test:types": "tsc --build tsconfig.json",
    "test:unit": "vitest run --config test/vite.config.js",
    "test:e2e": "start-server-and-test 'pnpm dev' http://localhost:3000 'pnpm cy:run'",
    "test:e2e:gui": "start-server-and-test 'pnpm dev' http://localhost:3000 'pnpm cy:open'",
    "coverage": "vitest --coverage",
    "prepare": "husky install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "benchmark": "node --loader ts-node/esm test/benchmarks/run.ts test/benchmarks/*.benchmark.*",
    "version:change": "beachball change",
    "version:bump": "beachball bump",
    "version:bump-edge": "pnpm jiti .github/scripts/bump-edge.ts",
    "version:check": "beachball check",
    "version:publish": "beachball publish"
  },
  "lint-staged": {
    "*.{js,json,css,vue}": "prettier --write",
    "*.{js,vue}": "eslint --fix"
  },
  "resolutions": {
    "node-fetch": "3.1.0"
  },
  "dependencies": {
    "@docus/mdc": "npm:@docus/mdc-edge@latest",
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest",
    "debounce": "^1.2.1",
    "pathe": "^0.2.0",
    "ufo": "^0.7.9",
    "unstorage": "^0.3.3",
    "ws": "^8.4.0"
  },
  "devDependencies": {
    "@nuxt/test-utils": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/benchmark": "^2.1.1",
    "@types/debounce": "^1.2.1",
    "@types/faker": "^5.5.9",
    "@types/object-hash": "^2.2.1",
    "@types/ws": "^8.2.2",
    "@unocss/nuxt": "^0.20.4",
    "beachball": "^2.20.0",
    "benchmark": "^2.1.4",
    "c8": "^7.10.0",
    "cypress": "^9.2.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "execa": "^6.0.0",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "nuxt3": "^3.0.0-27374259.21b664c",
    "object-hash": "^2.2.0",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "unbuild": "^0.6.7",
    "unocss": "^0.20.4",
    "vitest": "^0.0.107"
  },
  "bugs": {
    "url": "https://github.com/docusgen/docus/issues"
  }
}
