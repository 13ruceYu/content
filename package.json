    "h3": "^0.2.11",
    "hasha": "^5.2.2",
    "hookable": "^4.4.1",
    "jiti": "^1.10.1",
    "mande": "^1.0.0",
    "marked": "^2.1.3",
    "mkdirp": "^1.0.4",
    "ohmyfetch": "^0.1.8",
    "rehype-parse": "^7.0.1",
