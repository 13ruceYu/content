{
  "name": "@docus/core",
  "version": "1.2.2",
  "description": "The Docus core.",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module"
  ],
  "repository": "https://github.com/docusgen/core",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./runtime": {
      "require": "./dist/runtime.js",
      "import": "./dist/runtime.mjs"
    },
    "./dist/runtime/*": "./dist/runtime/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "runtime.mjs",
    "runtime.js",
    "runtime.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nuxt dev example",
    "generate": "nuxt generate example --force-build",
    "start": "nuxt start example",
    "lint": "eslint --ext .js,.ts,.vue .",
    "test": "yarn lint && jest --forceExit",
    "clean:example": "rm -rf example/.nuxt example/node_modules",
    "clean:dist": "rm -rf dist node_modules",
    "clean": "yarn clean:example && yarn clean:dist"
  },
  "dependencies": {
    "@docus/mdc": "npm:@docus/mdc-edge@latest",
    "@nuxt/bridge": "npm:@nuxt/bridge-edge@latest",
    "@nuxt/kit": "npm:@nuxt/kit-edge",
    "@lokidb/loki": "^2.1.0",
    "@nuxtjs/composition-api": "^0.28.0",
    "debounce": "^1.2.1",
    "defu": "^5.0.0",
    "h3": "^0.3.0",
    "hasha": "^5.2.2",
    "node-fetch": "^2.6.1",
    "object-hash": "^2.2.0",
    "property-information": "5.6.0",
    "scule": "^0.2.1",
    "ufo": "^0.7.9",
    "unctx": "^1.0.2",
    "unstorage": "0.2.3",
    "upath": "^2.0.1",
    "vue-docgen-api": "^4.41.1",
    "ws": "^7.5.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-runtime": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-typescript": "latest",
    "@nuxt/test-utils": "latest",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/debounce": "^1.2.0",
    "@types/jest": "latest",
    "@types/node-fetch": "^2.5.12",
    "@types/object-hash": "^2.1.1",
    "@types/ws": "^7.4.7",
    "babel-core": "^6.26.3",
    "babel-jest": "latest",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "latest",
    "nuxt-edge": "^2.16.0-27184333.6f94b1f9",
    "playwright": "^1.14.1",
    "prettier": "^2.3.2",
    "unbuild": "^0.4.2",
    "vue-jest": "^3.0.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
