    "babel-eslint": "latest",
    "babel-jest": "latest",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "latest",
    "playwright": "^1.12.3",
    "prettier": "^2.3.2",
    "siroc": "^0.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
