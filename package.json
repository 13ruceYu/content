    "@types/object-hash": "^2.2.1",
    "@types/ws": "^8.2.0",
    "babel-core": "^6.26.3",
    "chai": "^4.3.4",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "nuxt-edge": "^2.16.0-27276495.ab1c6cb4",
