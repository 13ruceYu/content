{
  "name": "docus",
  "version": "2.1.0",
  "description": "The Modern Website Generator.",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "docus-core"
  ],
  "repository": "https://github.com/docusgen/docus",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.mjs",
    "./runtime": "./dist/runtime.mjs",
    "./dist/runtime/*": "./dist/runtime/*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./types.d.ts",
  "files": [
    "dist",
    "types.d.ts",
    "shims.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "LOCAL_IMPORT=true nuxi dev example",
    "generate": "LOCAL_IMPORT=true nuxi generate example",
    "start": "nuxi start example",
    "lint": "eslint --ext .js,.ts,.vue .",
    "test": "yarn lint && mocha --timeout 10000 --exit -r jiti/register test/**/*.test.ts",
    "clean:example": "rm -rf example/.nuxt example/node_modules",
    "clean:dist": "rm -rf dist node_modules",
    "clean": "yarn clean:example && yarn clean:dist"
  },
  "dependencies": {
    "@docus/mdc": "npm:@docus/mdc-edge@latest",
    "@nuxt/image": "^0.6.0",
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest",
    "@nuxtjs/i18n": "^7.2.0",
    "chalk": "^5.0.0",
    "clear-module": "^4.1.2",
    "consola": "^2.15.3",
    "core-js": "3",
    "debounce": "^1.2.1",
    "defu": "^5.0.0",
    "glob": "^7.2.0",
    "h3": "^0.3.5",
    "iso-639-1": "^2.1.10",
    "jiti": "^1.12.9",
    "minimatch": "^3.0.4",
    "murmurhash-es": "^0.1.1",
    "nuxt-component-meta": "^0.0.7",
    "pathe": "^0.2.0",
    "property-information": "6.1.1",
    "scule": "^0.2.1",
    "ufo": "^0.7.9",
    "unctx": "^1.0.2",
    "unstorage": "^0.3.3",
    "ws": "^8.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "latest",
    "@babel/preset-typescript": "latest",
    "@docus/debug-theme": "latest",
    "@nuxt/bridge": "npm:@nuxt/bridge-edge@latest",
    "@nuxt/test-utils": "latest",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/chai": "^4.2.22",
    "@types/debounce": "^1.2.1",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node-fetch": "^3.0.3",
    "@types/ws": "^8.2.0",
    "chai": "^4.3.4",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "nuxt-edge": "^2.16.0-27298095.ab1c6cb4",
    "playwright": "^1.16.3",
    "prettier": "^2.5.0",
    "unbuild": "^0.5.13"
  }
}
