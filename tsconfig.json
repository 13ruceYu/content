{
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": "./",
    "strict": true,
    "target": "ESNext",
    "module": "CommonJS",
    "lib": ["ESNext", "DOM"],
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "declaration": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "types": ["./shims", "./types", "node", "jest", "@nuxt/kit", "@nuxt/nitro", "@nuxt/bridge"],
    "paths": {
      "#app": ["./node_modules/@nuxt/bridge/dist/runtime"]
    }
  },
  "vueCompilerOptions": {
    "experimentalCompatMode": 2
  }
}
